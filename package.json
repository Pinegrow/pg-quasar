{
  "name": "pg-quasar",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "Akayy <techakayy@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "dev:ios": "quasar dev -m ios",
    "dev:android": "quasar dev -m android",
    "now": "npm run build && npm run preview",
    "lighthouse:dev": "npx unlighthouse --no-cache --site http://localhost:9000",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-quasar-weather-app.netlify.app",
    "build": "quasar build",
    "build:electron": "quasar build -m electron",
    "build:ios": "quasar build -m ios",
    "build:android": "quasar build -m android",
    "preview": "quasar serve --history",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --ext .js,.ts,.vue ./ --fix  --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{js,ts,vue,css,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "icons": "icongenie g -i icon.png --theme-color 00B4DB --skip-trim",
    "vue-devtools": "vue-devtools"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "pinia": "^2.0.11",
    "quasar": "^2.8.0",
    "vue": "^3.2.29",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.176",
    "@pinegrow/quasar-module": "^3.0.36",
    "@quasar/app-vite": "^1.4.3",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@unocss/preset-icons": "^0.58.4",
    "@vue/devtools": "^6.5.1",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "unocss": "^0.58.4",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "web-types": "web-types.json"
}
